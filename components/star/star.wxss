@import '/assets/iconfont/iconfont.wxss';
.iconfont{
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

.m-star {
  position: relative;
  display: inline-block;
}
.star-wrappwer {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  letter-spacing: 0;
}
.star-score {
  position: absolute;
  left: 0;
  top: 0;
  color: orange;
  overflow: hidden;
}
.star-bg {
  color: #ddd;
}
.star-score i,
.star-bg i {
  margin-left: -0.5px;
}
.score {
  margin-left: 4px;
  vertical-align: middle;
}

.star-active {
  color: orange;
}
